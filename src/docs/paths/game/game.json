

{
	"get": {
		"summary": "This actually creates a game if not already existing.", 
		"tags": [
			"/game"
		], 
		"parameters": [
			{
				"name": "session_id", 
				"in": "query", 
				"required": "true", 
				"description": "ID of session that is being requested.", 
				"schema": {
					"type": "integer", 
					"example": 1
				}
			}
		], 
		"responses": {
			"204": {
				"description": "Players of a session."
			}, 
			"400": {
				"description": "SessionID invalid."
			}, 
			"401": {
				"description": "No token."
			}, 
			"403": {
				"description": "Forbidden", 
				"content": {
					"application/json": {
						"examples": {
							"token_payload_is_missing_id": {
								"summary": "Token payload is missing id.", 
								"value": {
									"message": "Token payload is missing id."
								}
							}, 
							"invalid_token": {
								"summary": "Invalid token.", 
								"value": {
									"message": "Invalid token."
								}
							}
						}
					}
				}
			}, 
			"404": {
				"description": "Not found.", 
				"content": {
					"application/json": {
						"examples": {
							"user_not_found": {
								"summary": "User not found.", 
								"value": {
									"message": "User not found."
								}
							}, 
							"session_not_found": {
								"summary": "Session not found.", 
								"value": {
									"message": "Session not found."
								}
							}, 
							"players_not_found": {
								"summary": "Players not found.", 
								"value": {
									"message": "Players not found."
								}
							}
						}
					}
				}
			}, 
			"500": {
				"description": "Internal Server Error"
			}
		}
	}, 
