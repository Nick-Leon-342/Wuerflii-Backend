

{
	"get": {
		"summary": "Get a specific final_score archive.", 
		"tags": [
			"/game/table_columns/archive"
		], 
		"parameters": [
			{
				"name": "session_id", 
				"in": "query", 
				"required": "true", 
				"description": "ID of session that is being requested.", 
				"schema": {
					"type": "integer", 
					"example": 1
				}
			}, {
				"name": "finalscore_id", 
				"in": "query", 
				"required": "true", 
				"description": "ID of final_score that is being requested.", 
				"schema": {
					"type": "integer", 
					"example": 342
				}
			}
		], 
		"responses": {
			"200": {
				"description": "Table_Archive of a Final_Score.", 
				"content": {
					"application/json": {
						"schema": {
							"$ref": "../../schemas/table_archive.json#/Table_Archive"
						}
					}
				}
			}, 
			"400": {
				"description": "Input invalid.", 
				"content": {
					"application/json": {
						"examples": {
							"sessionid_not_found": {
								"summary": "SessionID invalid.", 
								"value": {
									"message": "SessionID invalid."
								}
							}, 
							"finalscoreid_not_found": {
								"summary": "FinalScoreID invalid.", 
								"value": {
									"message": "FinalScoreID invalid."
								}
							}
						}
					}
				}
			}, 
			"401": {
				"description": "No token."
			}, 
			"403": {
				"description": "Forbidden", 
				"content": {
					"application/json": {
						"examples": {
							"token_payload_is_missing_id": {
								"summary": "Token payload is missing id.", 
								"value": {
									"message": "Token payload is missing id."
								}
							}, 
							"invalid_token": {
								"summary": "Invalid token.", 
								"value": {
									"message": "Invalid token."
								}
							}
						}
					}
				}
			}, 
			"404": {
				"description": "Not found.", 
				"content": {
					"application/json": {
						"examples": {
							"user_not_found": {
								"summary": "User not found.", 
								"value": {
									"message": "User not found."
								}
							}, 
							"session_not_found": {
								"summary": "Session not found.", 
								"value": {
									"message": "Session not found."
								}
							},
							"final_score_not_found": {
								"summary": "Final_Score not found.", 
								"value": {
									"message": "Final_Score not found."
								}
							},
							"table_archive_not_found": {
								"summary": "Table_Archive not found.", 
								"value": {
									"message": "Table_Archive not found."
								}
							}
						}
					}
				}
			}, 
			"500": {
				"description": "Internal Server Error"
			}
		}
	}
}
