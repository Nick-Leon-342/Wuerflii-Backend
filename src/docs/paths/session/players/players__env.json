

{
	"get": {
		"summary": "Get variables to create players.", 
		"tags": [
			"/session/players/env"
		], 
		"responses": {
			"200": {
				"description": "Variables for creating players.", 
				"content": {
					"application/json": {
						"schema": {
							"type": "object", 
							"required": [
								"MAX_PLAYERS", 
								"MAX_LENGTH_PLAYER_NAME"
							], 
							"properties": {
								"MAX_PLAYERS": {
									"type": "integer", 
									"example": 16
								}, 
								"MAX_LENGTH_PLAYER_NAME": {
									"type": "integer", 
									"example": 50
								}
							}
						}
					}
				}
			}, 
			"401": {
				"description": "No token."
			}, 
			"403": {
				"description": "Forbidden", 
				"content": {
					"application/json": {
						"examples": {
							"token_payload_is_missing_id": {
								"summary": "Token payload is missing id.", 
								"value": {
									"message": "Token payload is missing id."
								}
							}, 
							"invalid_token": {
								"summary": "Invalid token.", 
								"value": {
									"message": "Invalid token."
								}
							}
						}
					}
				}
			}, 
			"404": {
				"description": "User not found."
			}, 
			"500": {
				"description": "Internal Server Error"
			}
		}
	}
}
