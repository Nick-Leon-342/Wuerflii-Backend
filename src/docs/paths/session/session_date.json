

{
	"patch": {
		"summary": "Edit the view__custom_date of a specific session.", 
		"tags": [
			"/session/date"
		],
		"requestBody": {
			"required": "true", 
			"content": {
				"application/json": {
					"schema": {
						"type": "object", 
						"required": [
							"SessionID", 
							"View__Custom_Date"
						], 
						"properties": {
							"SessionID": { 
								"type": "integer", 
								"example": 1 
							},
							"View__Custom_Date": { 
								"type": "string", 
								"format": "date-time" 
							}
						}
					}
				}
			}
		}, 
		"responses": {
			"204": {
				"description": "Successfully edited view__custom_date." 
			}, 
			"400": {
				"description": "Input invalid.", 
				"content": {
					"application/json": {
						"examples": {
							"sessionid_invalid": {
								"summary": "SessionID invalid.", 
								"value": {
									"message": "SessionID invalid."
								}
							}, 
							"customdate_invalid": {
								"summary": "CustomDate invalid.", 
								"value": {
									"message": "CustomDate invalid."
								}
							}
						}
					}
				}
			}, 
			"401": {
				"description": "No token."
			}, 
			"403": {
				"description": "Forbidden", 
				"content": {
					"application/json": {
						"examples": {
							"token_payload_is_missing_id": {
								"summary": "Token payload is missing id.", 
								"value": {
									"message": "Token payload is missing id."
								}
							}, 
							"invalid_token": {
								"summary": "Invalid token.", 
								"value": {
									"message": "Invalid token."
								}
							}
						}
					}
				}
			}, 
			"404": {
				"description": "Not found.", 
				"content": {
					"application/json": {
						"examples": {
							"user_not_found": {
								"summary": "User not found.", 
								"value": {
									"message": "User not found."
								}
							}, 
							"session_not_found": {
								"summary": "Session not found.", 
								"value": {
									"message": "Session not found."
								}
							},
							"players_not_found": {
								"summary": "Players not found.", 
								"value": {
									"message": "Players not found."
								}
							}
						}
					}
				}
			}, 
			"500": {
				"description": "Internal Server Error"
			}
		}
	}
}
